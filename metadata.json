{
  "name": "rehan-cfssl",
  "version": "1.1.1",
  "author": "Rehan Mahmood",
  "summary": "A puppet module for installing, managing and generating SSL certificates using CloudFlare's PKI toolkit - CFSSL",
  "license": "Apache-2.0",
  "source": "https://github.com/rehanone/puppet-cfssl.git",
  "project_page": "https://github.com/rehanone/puppet-cfssl",
  "issues_url": "https://github.com/rehanone/puppet-cfssl/issues",
  "tags": [
    "cfssl",
    "ssl",
    "pki",
    "Public Key Infrastructure",
    "Certificate Managment"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian"
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "15.10",
        "16.04",
        "16.10",
        "17.04"
      ]
    },
    {
      "operatingsystem": "ArchLinux"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.0.0 < 5.0.0"
    },
    {
      "name": "rehan-wget",
      "version_requirement": ">= 1.0.0 < 2.0.0"
    }
  ],
  "data_provider": null
}
